{
  "name": "shops",
  "version": "1.0.0",
  "description": "Crawl webpage for shop data using https://github.com/fractalf/edderkopp",
  "scripts": {
    "install": "npm run build",
    "build": "babel --presets es2015 src -d bin",
    "watch": "babel --presets es2015 --watch src -d bin"
  },
  "dependencies": {
    "edderkopp": "git+https://github.com/fractalf/edderkopp.git",
    "minimist": "^1.2.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0"
  }
}
